/dts-v1/;
/plugin/;

#include <dt-bindings/gpio/gpio.h>

/{
	compatible = "brcm,bcm2835";

	fragment@0 {
		target = <&i2c_csi_dsi>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			gmsl0: gmsl-deserializer@27 {
				compatible = "maxim,max96712";
				reg = <0x27>;

				ports {
					#address-cells = <1>;
					#size-cells = <0>;

					port@6 {
						reg = <6>;
						max96712_out0: endpoint {
							remote-endpoint = <&csi1_ep>;
							clock-lanes = <0>;
							data-lanes = <1 2>;
							clock-noncontinuous;
						};
					};
				};
			};
		};
	};

	fragment@1 {
		target = <&csi1>;
		__overlay__ {
			status = "okay";

			port {
				csi1_ep: endpoint {
					remote-endpoint = <&max96712_out0>;
					clock-lanes = <0>;
					data-lanes = <1 2>;
					clock-noncontinuous;
				};
			};
		};
	};

	fragment@2 {
		target = <&i2c0if>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@3 {
		target = <&i2c0mux>;
		__overlay__ {
			status = "okay";
		};
	};
};
